{
  "name": "vite-plugin-svg-sprite-builder",
  "version": "1.0.0",
  "description": "A Vite plugin for building SVG sprites from icon directories",
  "homepage": "https://github.com/mango7j/vite-plugin-svg-sprite-builder#readme",
  "bugs": {
    "url": "https://github.com/mango7j/vite-plugin-svg-sprite-builder/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mango7j/vite-plugin-svg-sprite-builder.git"
  },
  "license": "MIT",
  "author": "mango7j",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "build-svg-sprite": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    }
  },
  "keywords": [
    "vite",
    "plugin",
    "svg",
    "sprite",
    "icons"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "pack": "npm pack",
    "test": "node test/core.test.js"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "fast-glob": "^3.3.2",
    "svgo": "^3.0.2",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}